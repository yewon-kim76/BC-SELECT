# Figure: histogram of Monte Carlo experiments
# Save null AUC values for each cases
control_t <- data.frame(control, rep("Control1", 5000)); colnames(control_t) <- c("AUC", "type")
bc1 <- data.frame(bc_metaAUC2, rep("Control2", 5000)); colnames(bc1) <- c("AUC", "type")
bc2 <- data.frame(bc_metaAUC4, rep("BC-SELECT", 5000)); colnames(bc2) <- c("AUC", "type")

DATA <- rbind(control_t, bc1, bc2)
auc <- data.frame(auc = c(AUC1, AUC2, AUC4), type = c("Control1","Control2", "BC-SELECT"))
ggplot(DATA, aes(x = AUC), fill = type) +
  geom_histogram(bins = 60) +  facet_grid(vars(type)) + 
  geom_vline(data=auc, aes(xintercept=auc, group = type, color="red"), linetype="dashed", size = 0.9) +
  theme(legend.position="None") + theme(strip.text = element_text(
    size = 11, color = "black")) + ylab("Count")


ggplot(data = df, aes(x=dataset, y = AUC, fill = method)) +
  geom_bar(stat="identity", position=position_dodge()) +
  theme(panel.border = element_rect(color = "black", fill = NA),  # Add border with color black
        panel.grid.major = element_blank(),  # Add major grid lines
        panel.grid.minor = element_blank(),  # Remove minor grid lines
        panel.background = element_blank(),
        legend.position = "bottom") + scale_fill_brewer(palette="Set1") +
  geom_text(aes(label = AUC, y = AUC), position = position_dodge(width = 1),
            vjust = -0.5, size = 5) + ylab("AUC of ROC")

